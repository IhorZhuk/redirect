{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "Controllers": "./src/app/controllers",
        "Components": "./src/app/components",
        "Reducers": "./src/app/reducers",
        "Pages": "./src/app/components/pages"
      }
    }]
  ]
}